<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".service.DocumentaryService">



    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:theme="@style/ShapeAppearance.Material3.LargeComponent"
        app:title="Service"
        app:titleTextColor="@color/white"
        app:navigationIcon="@drawable/back_pressed"
        android:id="@+id/documentary_service_toolbar_id"
        android:background="@color/service_bar"/>

    <WebView
        android:layout_marginTop="0dp"
        android:id="@+id/documentary_webview_id"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </WebView>

</LinearLayout>